<Window x:Class="CMDTNBB.Views.ShellView"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
        xmlns:vm="clr-namespace:CMDTNBB.ViewModels"
        mc:Ignorable="d"
        xmlns:cal="http://www.caliburnproject.org"
        d:DesignWidth="300" d:DesignHeight="300"
        d:DataContext="{d:DesignInstance Type=vm:ShellViewModel, IsDesignTimeCreatable=True}"
        cal:Bind.AtDesignTime="True"
        SizeToContent="WidthAndHeight">
    <Grid Background="White">
        <ContentControl Name="ItemVM" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" />
    </Grid>
</Window>